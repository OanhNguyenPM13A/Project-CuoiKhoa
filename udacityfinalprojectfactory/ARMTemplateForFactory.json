		{
			"name": "[concat(parameters('factoryName'), '/Dataflow1 Summary')]",
			"type": "Microsoft.DataFactory/factories/dataflows",
			"apiVersion": "2018-06-01",
			"properties": {
				"type": "MappingDataFlow",
				"typeProperties": {
					"sources": [
						{
							"dataset": {
								"referenceName": "snnycpayroll2020",
								"type": "DatasetReference"
							},
